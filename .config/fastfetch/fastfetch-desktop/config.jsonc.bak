{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "arch_small",
    "color": {"1": "94", "2": "94"},
    "padding": {
      "top": 1,
      "left": 2,
      "right": 2
    }
  },
  "display": {
    "separator": " ➜ ",
    "color": {"separator": "97"}
  },
  "modules": [
    {
      "type": "title",
      "format": "{6}{7}{8}",
      "color": {
        "user": "36",
        "at": "97",
        "host": "94"
      }
    },
    {
      "type": "os",
      "key": "OS ",
      "format": "{3}",
      "keyColor": "96"
    },
    {
      "type": "kernel",
      "key": "KER",
      "format": "{2}",
      "keyColor": "94"
    },
    {
      "type": "terminal",
      "key": "TER",
      "format": "{}",
      "keyColor": "96"
    },
    {
      "type": "shell",
      "format": "{}",
      "key": "SH ",
      "keyColor": "94"
    },
    {
      "type": "packages",
      "key": "PKG",
      "format": "{} (pacman)",
      "keyColor": "96"
    },
    {
      "type": "memory",
      "key": "MEM",
      "format": "{1} / {2}",
      "keyColor": "94"
    },
    {
      "type": "disk",
      "key": "DSK",
      "format": "{1} / {2}",
      "keyColor": "96"
    },
    "break",
    {
      "type": "custom",
      "format": "\u001b[93m󰮯 \u001b[94m󰧞 \u001b[91m \u001b[94m󰧞 \u001b[92m \u001b[94m󰧞 \u001b[94m \u001b[94m󰧞 \u001b[95m \u001b[94m󰧞 \u001b[96m \u001b[94m󰧞 \u001b[97m \u001b[94m󰧞 \u001b[90m"
    }
  ]
}
