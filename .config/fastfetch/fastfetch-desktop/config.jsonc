{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "small",
    // "width": 23,
    "padding": {
      "top": 2,
      "left": 3,
      "right": 2,
    },
  },
  "display": {
    "separator": " ➜ ",
    "color": { "separator": "97" },
    "key": {
      "type": "string",
    },
    "bar": {
      "width": 10,
      "border": {
        "left": "",
        "right": "",
      },
    },
  },
  "modules": [
    {
      "type": "custom",
      "format": "{#95}┏━━━━━━━━━━━━━━━━━━━━━━━┓",
    },

    {
      "type": "title",
      "format": "{#95}┃ {#31}▏▎▌▋▉ {6}{7}{8} {#31}▉🮉🮈🮇▕ {#95}┃",
      "color": {
        "user": "36",
        "at": "97",
        "host": "94",
      },
    },
    {
      "type": "os",
      "key": "┃ {#96}OS ",
      "format": "{3}      {#95}┃",
      "keyColor": "95",
    },
    {
      "type": "kernel",
      "key": "┃ {#94}KER",
      "format": "{2}  {#95}┃",
      "keyColor": "95",
    },
    {
      "type": "terminal",
      "key": "┃ {#96}TER",
      "format": "{}         {#95}┃",
      "keyColor": "95",
    },
    {
      "type": "shell",
      "key": "┃ {#94}SH ",
      "format": "{3} {4}         {#95}┃",
      "keyColor": "95",
    },
    {
      "type": "packages",
      "key": "┃ {#96}PKG",
      "format": "{2} (pacman)    {#95}┃",
      "keyColor": "95",
    },
    {
      "type": "memory",
      "key": "┃ {#94}MEM",
      // "format": "{4} {3}  {#95}┃",
      "format": "{4} {3}  \u001b[G\u001b[48C{#95}┃",
      "percent": {
        "type": 3,
        "green": 30,
        "yellow": 70,
      },
      "keyColor": "95",
    },
    {
      "type": "disk",
      "key": "┃ {#96}DSK",
      "folders": "/home",
      "format": "{13} {3}  {#95}┃",
      "percent": {
        "type": 7,
        "green": 30,
        "yellow": 70,
      },
      "keyColor": "95",
    },
    {
      "type": "custom",
      "format": "{#95}┃                       {#95}┃",
    },
    {
      "type": "custom",
      "format": "{#95}┃   {#31}■ {#32}■ {#33}■ {#34}■ {#35}■ {#36}■ {#37}■ {#38}■ {#39}■   {#95}┃",
    },

    {
      "type": "custom",
      "format": "{#95}┗━━━━━━━━━━━━━━━━━━━━━━━┛",
    },
  ],
}
