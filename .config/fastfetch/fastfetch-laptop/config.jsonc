{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "kitty",
    "source": "~/.config/fastfetch/thorn_logo.png",
    "width": 30,
    "padding": {
      "top": 2,
      "left": 1,
      "right": 1,
    },
  },
  "display": {
    "separator": " │",
    "color": {
      "separator": "green",
    },
    "key": {
      "type": "string",
    },
    "bar": {
      "width": 10,
      "border": {
        "left": "",
        "right": "",
      },
    },
  },
  "modules": [
    {
      "type": "custom",
      "format": "{#32}╭─────────────────────────╮",
    },

    {
      "type": "title",
      "format": "{#32}│       {6}{7}{8}{#32} \u001b[G\u001b[58C{#32}│",
      "color": {
        "user": "36",
        "at": "37",
        "host": "94",
      },
    },
    {
      "type": "custom",
      "format": "{#32}╰─────────────────────────╯",
    },
    {
      "type": "custom",
      "format": "{#32}╭─────╮╭──────────────────╮",
    },
    {
      "type": "os",
      "key": "{#32}│ {#93} OS",
      "format": "{#32}│ {#36}{3} \u001b[G\u001b[58C{#32}│",
      "keyColor": "95",
    },
    {
      "type": "kernel",
      "key": "{#32}│ {#93}KER",
      "format": "{#32}│ {#36}{2} \u001b[G\u001b[58C{#32}│",
      "keyColor": "95",
    },
    {
      "type": "terminal",
      "key": "{#32}│ {#93}TER",
      "format": "{#32}│ {#36}{} \u001b[G\u001b[58C{#32}│",
      "keyColor": "95",
    },
    {
      "type": "shell",
      "key": "{#32}│ {#93} SH",
      "format": "{#32}│ {#36}{3} {4} \u001b[G\u001b[58C{#32}│",
      "keyColor": "95",
    },
    {
      "type": "packages",
      "key": "{#32}│ {#93}PKG",
      "format": "{#32}│ {#36}{2} (pacman) \u001b[G\u001b[58C{#32}│",
      "keyColor": "95",
    },
    {
      "type": "memory",
      "key": "{#32}│ {#93}MEM",
      "format": "{#32}│ {4} {#35}{3} \u001b[G\u001b[58C{#32}│",
      "percent": {
        "type": 3,
        "green": 30,
        "yellow": 70,
      },
      "keyColor": "95",
    },
    {
      "type": "disk",
      "key": "{#32}│ {#93}DSK",
      "folders": "/home",
      "format": "{#32}│ {13} {#35}{3} \u001b[G\u001b[58C{#32}│",
      "percent": {
        "type": 7,
        "green": 30,
        "yellow": 70,
      },
      "keyColor": "95",
    },
    {
      "type": "battery",
      "key": "{#32}│ {#93}BAT",
      "format": "{#32}│ {10} {#35}{4} \u001b[G\u001b[58C{#32}│",
      "percent": {
        "type": 7,
        "green": 80,
        "yellow": 15,
      },
      "keyColor": "95",
    },
    {
      "type": "uptime",
      "key": "{#32}│ {#93} UP",
      "format": "{#32}│ {#36}{1}D : {2}H : {3}M \u001b[G\u001b[58C{#32}│",
    },
    {
      "type": "custom",
      "format": "{#32}╰─────╯╰──────────────────╯",
    },
    {
      "type": "custom",
      "format": "{#32}╭─────────────────────────╮",
    },
    {
      "type": "custom",
      "format": "{#32}│    {#91}■ {#32}■ {#93}■ {#34}■ {#35}■ {#36}■ {#37}■ {#38}■ {#39}■   {#32} │",
    },

    {
      "type": "custom",
      "format": "{#32}╰─────────────────────────╯",
    },
  ],
}
